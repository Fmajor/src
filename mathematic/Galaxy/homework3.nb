Cell[CellGroupData[{

Cell["2.Ram pressure stripping", "Section",
 CellChangeTimes->{{3.5290221208166428`*^9, 3.529022146595117*^9}}],

Cell[CellGroupData[{

Cell["\<\
a) What is the verticle force per unit mass...\
\>", "Subsection",
 CellChangeTimes->{{3.5290222826608996`*^9, 3.5290222895492935`*^9}, 
   3.529022735267787*^9, {3.5290254825089207`*^9, 3.5290254827839365`*^9}, {
   3.5290271383096266`*^9, 3.5290271394876943`*^9}}],

Cell[TextData[{
 "Solution:\n\tVerticle force of a particle at the distance z from the sheet. \
Use the polar coordinate, the force per unit mass\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "G"], "=", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
     RowBox[{"G", " ", 
      FractionBox[
       RowBox[{"\[CapitalSigma]", " "}], 
       SuperscriptBox["R", "2"]], "2", "\[Pi]", " ", "r", " ", 
      RowBox[{"cos", "(", "\[Theta]", ")"}], 
      RowBox[{"\[DifferentialD]", "r"}]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\tand cos \[Theta] = ",
 Cell[BoxData[
  FormBox[
   FractionBox["z", 
    SqrtBox[
     RowBox[{
      SuperscriptBox["z", "2"], "+", 
      SuperscriptBox["r", "2"]}]]], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{"R", "=", 
    SqrtBox[
     RowBox[{
      SuperscriptBox["z", "2"], "+", 
      SuperscriptBox["r", "2"]}]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["F", "G"], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], " ", 
      RowBox[{
       FractionBox[
        RowBox[{
        "G", " ", "\[CapitalSigma]", " ", "2", "\[Pi]", " ", "r", " ", "z"}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["z", "2"], "+", 
           SuperscriptBox["r", "2"]}], ")"}], 
         RowBox[{"3", "/", "2"}]]], 
       RowBox[{"\[DifferentialD]", "r"}]}]}], "=", 
     RowBox[{"2", "G\[Pi]\[CapitalSigma]"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " (z\[NotEqual]0)"
}], "Text",
 CellChangeTimes->{{3.5290226546541767`*^9, 3.529022726665295*^9}, {
  3.529025983217559*^9, 3.529026008125984*^9}, {3.529027096460233*^9, 
  3.529027220410323*^9}, {3.529027262038704*^9, 3.529027619090126*^9}, {
  3.529027650495922*^9, 3.5290276791905637`*^9}, {3.5290277304184933`*^9, 
  3.529027735139764*^9}, {3.529028874144911*^9, 3.5290288774791017`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"FG", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"G", " ", 
     RowBox[{"\[CapitalSigma]", " ", "/", 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["r", "2"], "+", 
        SuperscriptBox["z", "2"]}], ")"}]}], "*", "2", "\[Pi]", " ", "r", " ", 
     RowBox[{"z", "/", 
      SqrtBox[
       RowBox[{
        SuperscriptBox["r", "2"], "+", 
        SuperscriptBox["z", "2"]}]]}]}], ",", 
    RowBox[{"{", 
     RowBox[{"r", ",", "0", ",", "\[Infinity]"}], "}"}]}], "]"}]}]], "Code",
 CellChangeTimes->{{3.5290260479602623`*^9, 3.5290260888636017`*^9}, {
  3.529026127576816*^9, 3.5290261874372396`*^9}, {3.529026302374814*^9, 
  3.5290263204958506`*^9}, {3.5290263519156475`*^9, 3.529026363228295*^9}, {
  3.5290263938870487`*^9, 3.529026394739097*^9}, {3.529026561538637*^9, 
  3.52902656279871*^9}, {3.529026602515981*^9, 3.5290266219980955`*^9}, {
  3.5290266972133975`*^9, 3.529026711922239*^9}, {3.5290268402705803`*^9, 
  3.5290268667710953`*^9}, {3.52902763104881*^9, 3.5290276402943387`*^9}}],

Cell[BoxData[
 RowBox[{"ConditionalExpression", "[", 
  RowBox[{
   FractionBox[
    RowBox[{"2", " ", "G", " ", "\[Pi]", " ", "z", " ", "\[CapitalSigma]"}], 
    SqrtBox[
     SuperscriptBox["z", "2"]]], ",", 
   RowBox[{
